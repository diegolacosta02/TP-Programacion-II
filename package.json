{
  "name": "cuvl_lab3",
  "version": "1.0.0",
  "description": "cuvl_lab3",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc",
    "clean": "npm run clean:build && npm run clean:temp",
    "clean:build": "npx rimraf ./dist",
    "clean:temp": "npx rimraf ./tmp",
    "dev-mode": "ts-node ./src/index.ts",
    "lint": "npx eslint ./src",
    "start": "node ./dist/index.js",
    "start:dev": "tsc && node ./dist/index.js",
    "test": "npx jest tests --runInBand --detectOpenHandles",
    "prebuild": "npm run clean",
    "lint-fix": "npx eslint ./src --fix",
    "docs": "jsdoc -c jsdoc.json"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-compact-reporter": "^1.2.9",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^4.0.0",
    "jsdoc": "^4.0.5",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "license": "ISC"
}
